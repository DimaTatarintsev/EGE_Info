#4
'''
count = 0
digits12 = '0123456789AB'
odd = '13579B'

for a in digits12:
    if a == '0': continue 
    for b in digits12:
        for c in digits12:
            for d in digits12:
                for e in digits12:
                    num = a + b + c + d + e
                    pairs = 0
                    for i in range(4):
                        if num[i] in odd and num[i+1] in odd:
                            pairs += 1
                    if pairs <= 2:
                        count += 1
print("4. Ответ:", count)
