#1

k = 0
v=[]
w = 'АКОРСТ'
for a in w:
    for b in w:
        for c in w:
            for d in w:
                for e in w:
                        word = a+b+c+d+e
                        k += 1
                        if k % 2 == 0 and a != 'А' and a != 'С' and a != 'Т' and word.count('О')==2:
                            v.append((k,word))   
                            print(f'{k} {word}')


