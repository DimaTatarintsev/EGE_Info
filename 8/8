#5
'''
even_d = '02468'
odd_d = '13579'

def valid_num(n):
    s = str(n)
    for i in range(4):
        if (s[i] in even_d and s[i+1] in even_d) or (s[i] in odd_d and s[i+1] in odd_d):
            return False
    return True

numbers = []
for num in range(10000, 100000):
    if valid_num(num):
        numbers.append(num)

last_index = -1
for idx, val in enumerate(numbers, start=1):
    if idx % 15 == 0:
        last_index = idx
print("5. Номер последнего кратного 15:", last_index)
'''
#6
'''
from itertools import product
digits16 = '0123456789ABCDEF'
digits38 = '0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ'

count = 0
for d0, d1, d2, d3, d5 in product(digits16, repeat=5):
    if d0 == '0':
        continue
    s16 = d0 + d1 + d2 + d3 + 'A' + d5
    n = int(s16, 16)
    tmp = n
    digits = []
    while tmp > 0:
        digits.append(digits38[tmp % 38])
        tmp //= 38
    if len(digits) != 7:
        continue
    digits.reverse()
    if digits[4] != '2':
        continue
    if digits[0] == '0':
        continue
    count += 1

print("6. Ответ:", count)
'''
