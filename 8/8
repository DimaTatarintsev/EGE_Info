#3
'''
count = 0
digits7 = '0123456'
even = '0246'

for a in digits7:
    if a == '0': continue   
    for b in digits7:
        for c in digits7:
            for d in digits7:
                for e in digits7:
                    num = a + b + c + d + e
   
                    three_even = False
                    for i in range(3):
                        if num[i] in even and num[i+1] in even and num[i+2] in even:
                            three_even = True
                            break
                    if three_even:
                        continue
х
                    pairs = 0
                    for i in range(4):
                        if num[i] in even and num[i+1] in even:
                            pairs += 1
                    if pairs >= 2:
                        count += 1
print("3. Ответ:", count)
