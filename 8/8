'''
k = 0
w = '01234' 
even = '024'  
valid_numbers = []

for a in w:
    for b in w:
        for c in w:
            for d in w:
                for e in w:
                    for f in w:
                        for g in w:
                            for h in w:
                                for i in w:
                                    num = a + b + c + d + e + f + g + h + i
                                    k += 1
                                    pairs_count = 0
                                    for pos in range(8):
                                        if num[pos] in even and num[pos+1] in even:
                                            pairs_count += 1
                                    has_three_even = False
                                    for pos in range(7):
                                        if (num[pos] in even and 
                                            num[pos+1] in even and 
                                            num[pos+2] in even):
                                            has_three_even = True
                                            break
                                    if pairs_count == 2 and not has_three_even:
                                        valid_numbers.append(num)
                        
print(f"Количество чисел: {len(valid_numbers)}")
