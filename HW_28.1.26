#2

f = open('9_7335.xls', 'r')
c = 0
for i in f.readlines():
    a = list(map(int, i.strip().split(';')))
    
    c1 = False
    if len(set(a)) == 4:
        avg = (min(a) + max(a)) / 2
        ip = int(avg)
        if ip in a:
            c1 = True
    
    c2 = False
    d = {}
    for x in a:
        d[x] = d.get(x, 0) + 1
    
    r = [x for x, cnt in d.items() if cnt > 1]
    nr = [x for x, cnt in d.items() if cnt == 1]
    
    if r:
        sr = sum(x**2 for x in r)
        snr = sum(x**2 for x in nr)
        if sr < snr:
            c2 = True
    
    if (c1 and not c2) or (c2 and not c1):
        c += 1
print(c)

