c = 0
for i in f.readlines():
    a = list(map(int, i.strip().split(';')))
    d = {}
    for x in a:
        d[x] = d.get(x, 0) + 1
    
    p = [x for x, cnt in d.items() if cnt == 2]
    u = [x for x, cnt in d.items() if cnt == 1]
    
    if len(p) == 2 and len(u) == 1:
        p1, p2 = sorted(p)
        v = u[0]
        if p1 < v < p2:
            c += 1
print(c)

#6

f = open('9_8365.csv', 'r')
c = 0
from itertools import combinations
for i in f.readlines():
    a = list(map(int, i.strip().split(';')))
    
    if len(set(a)) != 5:
        continue
    
    fnd = False
    for s in a:
        r = [x for x in a if x != s]
        prs = list(combinations(r, 2))
        
        for j in range(len(prs)):
            for k in range(j+1, len(prs)):
                p1, p2 = prs[j], prs[k]
                if len(set(p1 + p2)) != 4:
                    continue
                a1 = sum(p1)/2
                a2 = sum(p2)/2
                if abs(a1-s) < 1e-9 and abs(a2-s) < 1e-9:
                    fnd = True
                    break
            if fnd: break
        if fnd: break
    
    if fnd:
        c += 1
print(c)