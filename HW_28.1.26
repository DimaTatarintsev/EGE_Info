f = open('9_8554.xlsx', 'r')
c = 0
for i in f.readlines():
    a = list(map(int, i.strip().split(';')))
    
    t = sum(1 for x in a if abs(x) % 10 == 3)
    if t != 3:
        continue
    
    ps = sum(x for x in a if x > 0)
    ns = sum(x for x in a if x < 0)
    
    if ps**2 < ns**2:
        c += 1
print(c)

