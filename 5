def make_n(n, p):
	st=''
	while n>0:
		st += str(n%p)
		n //= p
	return st[::-1]
print(make_n(6,3))

def sm_u(n):
	res = 0
	for x in n:
		res += int(x)
	return(res)		
arr = []
for n in range(3,1000):
	n3 = make_n(n,3)
	if n%3 == 0:
		n3 = n3 + n3[-2:]
	else:
		sm = sm_u(n3)
		sm = sm*3
		sm3 = make_n(sm, 3)
		n3 = n3 + sm3
	r = int(n3, 3)
	if r%2 != 0 and r > 208:
		arr.append((r, n))
arr.sort()
print(arr[:10])